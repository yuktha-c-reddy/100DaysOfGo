package main
import ("fmt"
        "math/rand"
        "net/http"
        "time")

func main(){
    rand.Seed(time.Now().UnixNano())
    http.HandleFunc("/",home)
    http.ListenAndServe(":8080",nil)
}

func home(w http.ResponseWriter , r *http.Request){
    fmt.Fprintln(w,"Welcome to URl Shortener")
}
